{
    "name": "softspring/translatable-bundle",
    "description": "Symfony form type for translatable fields",
    "type": "symfony-bundle",
    "license": "AGPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "^2.13|^3.2|^4.2",
        "doctrine/orm": "^2.19|^3.3",
        "symfony/doctrine-bridge": "^5.4|^6.0|^7.0",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0",
        "twig/twig": "^2.16|^3.14"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.68",
        "google/cloud-translate": "^2.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "^1.0",
        "softspring/dynamic-form-type": "^5.4",
        "symfony/framework-bundle": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\TranslatableBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\TranslatableBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
